@model StreamShift.Domain.Entities.Transfer
@{
    ViewData["Title"] = "DeleteComplate";
}

<h1>Silme İşlemi Tamamlandı</h1>

@if (TempData["SuccessMessage"] != null)
{
    <div class="alert alert-success" role="alert">
        @TempData["SuccessMessage"]
    </div>
}
else{
    <div class="alert alert-success" role="alert">
        @TempData["Silme işlemi gerçekleştirilemedi lütfen tekrar deneyiniz."]
    </div>
}

<div class="form-group">
    <a asp-action="Index" class="btn btn-primary">Geri Dön</a>
</div>